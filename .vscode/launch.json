{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach Site",
      "port": 9222,
      "url": "http://localhost:9000",
      "urlFilter": "http://localhost:9000/*",
      "webRoot": "${workspaceRoot}/dist",
      "sourceMaps": true,
      "trace": true
    },
    {
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "webpack: dev server",
      "name": "Launch Site",
      "url": "http://localhost:9000"
    }
  ]
}
